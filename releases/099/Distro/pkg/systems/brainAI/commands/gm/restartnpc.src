use uo;

include "include/security";

program textcmd_fixAI(who)

        if( !AuthorizationCode( who ))
                SendSysMessage( who, "Cancelled." );
                return 0;
        endif
        
	var targ := target(who);
	var result := RestartScript(targ);
	
	SendSysMessage(who, "Result of RestartScript("+targ.name+") ->"+result);
	
	return 1;
endprogram
