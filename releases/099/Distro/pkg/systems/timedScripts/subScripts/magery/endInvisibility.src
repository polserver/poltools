use uo;
use os;

include ":attributes:attributes";

program TimedScript_EndInvisibility( params )

	var mobile := params[1];
	    params := 0;
	
	EraseObjProperty( mobile, "Invisibility" );
	mobile.hidden := 0;
	
	return 1;
endprogram

