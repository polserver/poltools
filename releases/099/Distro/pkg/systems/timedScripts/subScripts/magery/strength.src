use uo;
use os;

include ":attributes:attributes";

program TimedScript_Strength( params )

	var defender := params[1],
	    //attacker := params[2],
	    points := params[3];
	    params := 0;
	
	SetObjProperty( defender, "StrengthMod", points );
 
	AP_ModifyStatMod( defender, STRENGTH, points );
	
	return 1;
endprogram

