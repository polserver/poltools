use os;
use uo;

program textcmd_viewBounty( mobile )

        var start := start_script( ":bountyBoard:board/use", mobile );
        if( start.errortext )
                   SendSysMessage( mobile, "Error cannot start script <:bountyBoard:board/use> -->"+start.errortext );
        endif
        
        SendSysMessage( mobile, "Done!" );
        
        return 1;
endprogram
