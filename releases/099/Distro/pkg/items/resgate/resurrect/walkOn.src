use uo;
use os;

include ":resgate:resgate";

program walkon( mobile, tile )

	// Used to fix unused var report by ecompile: tile;
	tile := tile;

	if( mobile.dead )
                ResurrectGate( mobile );
	else
		SendSysMessage( mobile, "You are at peace..." );
	endif

	return 1;
endprogram
