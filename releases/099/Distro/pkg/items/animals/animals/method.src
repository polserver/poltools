use uo;
use cfgfile;

program Install()

	return 1;
endprogram

exported function IsAnimal( item )
	item := item; // Avoid compiler warnings.
	
	return 1;
endfunction

exported function NPCTemplate( item )

        var cfg := ReadConfigFile( ":*:itemdesc" ),
            template := cfg[item.objtype].NPCTemplate;

	return template;
endfunction
