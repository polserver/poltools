use uo;
use cfgfile;

program Install()

	return 1;
endprogram

exported function GetHouseName( deed )

	var cfg_file := ReadConfigFile( ":*:itemdesc" ), // package itemdesc
	    elem := cfg_file[deed.objtype],
            name := CStr( elem.HouseObjName );
 
	return name;
endfunction
