use uo;
use os;

program console_online(cmd)
	cmd := cmd;
		var online := EnumerateOnlineCharacters();
		if ( online.Size() > 0 )
			foreach person in ( online )
				Print ("("+_person_iter+") Account:"+person.acctname+" Character:"+person.name);
				SleepMS(2);
			endforeach
		else
			Print("There is no one online.");
		endif
	
	return 1;
endprogram
