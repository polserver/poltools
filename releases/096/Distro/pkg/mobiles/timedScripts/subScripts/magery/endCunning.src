use uo;
use os;

include ":attributes:attributes";

program TimedScript_EndCunning(params)
	var mobile := params[1];
	
	var points := CInt(GetObjProperty(mobile, "#CunningMod"));	
	AP_ModifyStatMod(mobile, INTELLIGENCE, -(points));
	EraseObjProperty(mobile, "#CunningMod");
	
	return 1;
endprogram

