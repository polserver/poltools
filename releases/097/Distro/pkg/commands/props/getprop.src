use uo;

program propertyinfo(who, property)
	var targ := target(who);
	if ( !targ )
		SendSysMessage(who, "Cancelled");
		return 0;
	endif
	SendSysMessage(who, "Property "+property+": "+targ.Get_Member(property));
endprogram
