use uo;
include "include/attributes";

program textcmd_setint(chr)
    var who := Target(chr);
    SetDexterityMod(who, 0);
    SetIntelligenceMod(who, 0);
    SetStrengthMod(who, 0);
    who.ar_mod := 0;
    foreach thing in GetAttributeIds()
      SetAttributeTemporaryMod(chr, thing, 0);
    endforeach
endprogram
